{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Title: Metagenomic Analysis of Soil Microbial Communities in Agricultural Fields: Impact of Fertilization on Functional Diversity",
  "type": "object",
  "properties": {
    "project_name": {
      "type": "string",
      "description": "Name of the project",
      "default": "Title: Metagenomic Analysis of Soil Microbial Communities in Agricultural Fields: Impact of Fertilization on Functional Diversity"
    },
    "investigation_type": {
      "type": "string",
      "description": "Type of investigation",
      "default": "genome"
    },
    "collection_date": {
      "type": "string",
      "description": "Date of sample collection"
    },
    "geo_loc_name": {
      "type": "string",
      "description": "Geographic location",
      "default": "USA:Iowa"
    },
    "lat_lon": {
      "type": "string",
      "description": "Latitude and longitude",
      "default": "42.0308 -93.6319"
    },
    "env_biome": {
      "type": "string",
      "description": "Environmental biome",
      "default": "terrestrial biome"
    },
    "env_material": {
      "type": "string",
      "description": "Environmental material",
      "default": "soil"
    },
    "sample_collect_device": {
      "type": "string",
      "description": "Sample collection device"
    },
    "seq_meth": {
      "type": "string",
      "description": "Sequencing method"
    },
    "depth": {
      "type": "number",
      "description": "Depth in meters"
    },
    "temp": {
      "type": "number",
      "description": "Temperature in Celsius"
    },
    "ph": {
      "type": "number",
      "description": "pH value"
    }
  },
  "required": [
    "project_name",
    "investigation_type",
    "collection_date",
    "geo_loc_name"
  ]
}
